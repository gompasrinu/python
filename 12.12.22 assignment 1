from tkinter import*
fb=Tk( )
fb. title("student form")
Label(fb,text="enter student register id:").grid(row=0)
Label(fb,text=" enter student name:").grid(row=1)
Label(fb,text=" enter father name:").grid(row=2)
Label(fb,text=" enter group:").grid(row=3)
Label(fb,text="enter phn.no:").grid(row=4)
Label(fb,text="enter email:").grid(row=5)
Entry(fb).grid(row=0,column=1)
Entry(fb).grid(row=1,column=1)
Entry(fb).grid(row=2,column=1)
Entry(fb).grid(row=3,column=1)
Entry(fb).grid(row=4,column=1)
Entry(fb).grid(row=5,column=1)
Button(fb,text="login").grid(row=6)
Button(fb,text="cancel").grid(row=6,column=1)
mainloop( )
